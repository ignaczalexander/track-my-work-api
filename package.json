    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.18",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.1",
    "query-string": "^6.8.3",
